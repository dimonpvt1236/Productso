{
    "name": "dimonpvt/productso",
    "type": "library",
    "description": "Manage products and categories in Laravel 5 with Sleeping-owl admin panel",
    "keywords": [
        "dimonpvt1236",
        "productso"
    ],
    "homepage": "https://github.com/dimonpvt1236/Productso",
    "license": "MIT",
    "authors": [
        {
            "name": "dimonpvt1236",
            "email": "dimonpvt@mail.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.2",
        "php" : "~5.5|~7.0",
        "kalnoy/nestedset": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Dimonpvt\\Productso\\": "src/",
            "Dimonpvt\\Productso\\Http\\Controllers\\": "src/Http/Controllers/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dimonpvt\\Productso\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
